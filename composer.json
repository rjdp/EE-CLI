{
	"name": "rjdp/ee-cli",
	"description": "A CLI Cpanel to manage WordPress sites running on Nginx",
	"keywords": [ "cli", "wordpress", "nginx"],
	"license": "MIT",
	"require": {
		"php": ">=5.3.2",
		"wp-cli/php-cli-tools": "dev-master",
		"mustache/mustache": "~2.4",
		"ramsey/array_column": "~1.1",
		"rmccue/requests": "~1.6",
		"symfony/finder": "~2.3",
		"nb/oxymel": "0.1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*",
		"behat/behat": "2.5.*"
	},
	"suggest": {
		"psy/psysh": "Enhanced `wp shell` functionality"
	},
	"autoload": {
		"psr-0": { "EE_CLI": "php" }
	}
}
